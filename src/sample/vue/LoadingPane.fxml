<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="pane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="100.0" prefWidth="100.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="sample.control.util.LoadingController">
	<center>
		<ProgressIndicator BorderPane.alignment="CENTER" />
	</center>
	<bottom>
		<Label fx:id="wait" text="Label" BorderPane.alignment="CENTER">
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Label>
	</bottom>
</BorderPane>
