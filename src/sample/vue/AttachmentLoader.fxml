<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckMenuItem?>
<?import javafx.scene.control.CustomMenuItem?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="65.0"
	prefWidth="70.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="sample.control.load.AttachmentLoaderController">
	<right>
		<MenuButton alignment="CENTER" contentDisplay="CENTER"
			ellipsisString="" mnemonicParsing="false" prefHeight="65.0"
			prefWidth="9.0" text="MenuButton" BorderPane.alignment="CENTER">
			<items>
				<MenuItem fx:id="delete" mnemonicParsing="false"
					onAction="#delete" text="Action" />
				<CheckMenuItem fx:id="toUpload"
					mnemonicParsing="false" selected="true" text="Action" />
				<Menu fx:id="source" mnemonicParsing="false"
					text="Unspecified Menu">
					<items>
						<CustomMenuItem mnemonicParsing="false"
							text="Action">
							<content>
								<Text fx:id="sourceText" strokeType="OUTSIDE"
									strokeWidth="0.0" />
							</content>
						</CustomMenuItem>
					</items>
				</Menu>
			</items>
			<font>
				<Font size="7.0" />
			</font>
		</MenuButton>
	</right>
	<center>
		<Text fx:id="extension" strokeType="OUTSIDE" strokeWidth="0.0"
			text="EXE" BorderPane.alignment="CENTER">
			<font>
				<Font name="Arial Black" size="16.0" />
			</font>
		</Text>
	</center>
</BorderPane>
